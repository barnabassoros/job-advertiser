// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model ad {
  id          String   @id @default(uuid())
  time        DateTime
  location    String
  duration    String
  payment     Int
  description String
}
